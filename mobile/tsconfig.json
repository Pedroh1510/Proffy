{
  "compilerOptions": {
    "esModuleInterop":true,
    "target": "es2017",
    "module": "CommonJS",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
  }
}
